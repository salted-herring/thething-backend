{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"mobiledetect/mobiledetectlib": "~2.8.12",
		
		"silverstripe/cms": "~3.3.0",
		"silverstripe/framework": "~3.3.0",
		"silverstripe/googlesitemaps": "~1.2.2",
		"silverstripe/html5": "~1.0.2",
		"silverstripe-australia/gridfieldextensions": "~1.1.0",
		"silverstripe/staticpublisher": "~1.0.0",
		"silverstripe/lumberjack": "~1.1",
		
		"hubertusanton/silverstripe-seo": "~2.0",
		"kinglozzer/silverstripegmapsobject": "~1.0.1",
		"heyday/silverstripe-optimisedimage": "~2.2.1",
		"heyday/silverstripe-dataobjectpreview": "~1.0.2",
		"icecaster/versioned-gridfield": "dev-master",
		"normann/gridfieldpaginatorwithshowall": "dev-master",
		"ntb/silverstripe-rest-api": "dev-master"
